<p id="notice"><%= notice %></p>

<h1>Health Warnings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health_warnings.each do |health_warning| %>
      <tr>
        <td><%= health_warning.name %></td>
        <td><%= health_warning.link %></td>
        <td><%= link_to 'Show', health_warning %></td>
        <td><%= link_to 'Edit', edit_health_warning_path(health_warning) %></td>
        <td><%= link_to 'Destroy', health_warning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health Warning', new_health_warning_path %>
